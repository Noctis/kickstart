{
    "name": "noctis/kickstart",
    "license": "proprietary",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:NoctisPL/database-lib.git"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "nikic/fast-route": "^1.3",
        "noctis/database-lib": "dev-future/v2",
        "paragonie/easydb": "^2.11",
        "php-di/php-di": "^6.3",
        "psr/http-server-middleware": "^1.0",
        "symfony/console": "^5.2",
        "symfony/http-foundation": "^5.2",
        "twig/twig": "^3.2",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^5.2",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Noctis\\KickStart\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
