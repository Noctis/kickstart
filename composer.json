    "require": {
        "php": "^7.3",
        "ext-pdo": "*",
        "darsyn/ip": "^4.0",
        "nikic/fast-route": "^1.3",
        "paragonie/easydb": "^2.11",
        "php-di/php-di": "^6.0",
        "psr/http-server-middleware": "^1.0",
        "symfony/console": "^5.0",
